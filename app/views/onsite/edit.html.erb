<div class="container">
  <%= link_to '<< Back', onsite_index_path, class: "btn btn-secondary" %>

  <h1 align="center">Edit On Site Service</h1>

  <%= form_for @onsite, url: { action: "update", params: { id: @onsite.id } } do |f| %>
    <%= render partial: 'form', locals: { f: f } %>

    <div align="right">
      <%= f.submit "Save On Site Service", class: "btn btn-success" %>
    </div>
  <% end %>
</div>
