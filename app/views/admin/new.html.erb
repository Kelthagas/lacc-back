<button class="btn btn-light" id="backButton"><i class="fa fa-arrow-left" aria-hidden="true"></i></button>
<h1 class="text-center">Create New User</h1>

<%= form_for @user, url: { action: "create", params: { id: @user.id } } do |f| %>
  <%= render partial: 'form', locals: { f: f } %>
  <div>
    <%= f.submit "Create User", class: "btn btn-success" %>
  </div>
<% end %>
