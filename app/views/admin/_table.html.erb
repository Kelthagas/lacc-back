<h2><%= "#{ header }" %></h2>
<table class="table">
  <thead>
    <tr>
      <th>Category</th>
      <th>Sub Category</th>
      <th>Value</th>
      <th>Options</th>
    </tr>
  </thead>
  <tbody>
      <% data.each do |d| %>
        <tr>
          <td><%= d.category %></td>
          <td><%= d.sub_category %></td>
          <td><%= d.value %></td>
          <td>
            <%= link_to 'Edit', '#', class: "btn btn-secondary" %>
            <%= link_to 'Delete', '#', class: "btn btn-danger" %>
          </td>
        </tr>
      <% end %>
  </tbody>
</table>
